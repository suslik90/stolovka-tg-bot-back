<body style="margin: 0; padding: 0 !important; mso-line-height-rule: exactly; background-color: #f1f1f1;"
  cz-shortcut-listen="true">
  <center style="background-color: #f1f1f1;">
    <div
      style="display: none; font-size: 1px;max-height: 0px; max-width: 0px; opacity: 0; overflow: hidden; mso-hide: all; font-family: sans-serif;">
      ‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;‌&nbsp;
    </div>
    <div style="max-width: 600px; min-width: 550px; margin: 0 auto;" class="email-container">

      <table align="center" role="presentation" cellspacing="0" cellpadding="0" border="0"
        style="margin: auto;">
        <tbody>
          <tr>
            <td valign="top" class="bg_white" style="padding: 2.5em 2.5em 0 2.5em;">
              <table role="presentation" border="0" cellpadding="0" cellspacing="0" width="100%">
                <tbody>
                  <tr>
                    <td style="text-align: left;" colspan="2">
                      <h2 class="margin-end-header">Доставка</h2>
                    </td>
                  </tr>
                  <tr>
                    <td>Город:</td>
                    <td>{{delivery.city}}</td>
                  </tr>
                  <tr>
                    <td>Улица и дом:</td>
                    <td>{{delivery.street}}</td>
                  </tr>
                  <tr>
                    <td>№ квартиры/офиса:</td>
                    <td>{{delivery.apartment}}</td>
                  </tr>
                  <tr>
                    <td>Подъезд:</td>
                    <td>{{delivery.entrance}}</td>
                  </tr>
                  <tr>
                    <td>Этаж:</td>
                    <td>{{delivery.level}}</td>
                  </tr>
                  <tr>
                    <td>Имя:</td>
                    <td>{{delivery.name}}</td>
                  </tr>
                  <tr>
                    <td>Телефон:</td>
                    <td>{{delivery.phone}}</td>
                  </tr>
                  <tr>
                    <td>Комментарий:</td>
                    <td>{{delivery.comment}}</td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr>
          </tr>
        </tbody>
      </table>
      <table align="center" role="presentation" cellspacing="0" cellpadding="0" border="0"
        style="margin: auto;">
        <tbody>
          <tr>
            <td valign="top" class="bg_white" style="padding: 1em 2.5em 0 2.5em;">
              <table role="presentation" border="0" cellpadding="0" cellspacing="0" width="100%">
                <tbody>
                  <tr>
                    <td style="text-align: left;" class="order-item-name">
                      <h2 class="margin-end-header">Заказ</h2>
                    </td>
                    <td colspan="3"></td>

                  </tr>
                   {{#each order}}
                  <tr>
                    <td>{{this.name}}</td>
                    <td align="right">{{this.count}}</td>
                    <td align="right">{{this.price}}р</td>
                    <td align="right">{{this.totalPrice}}р</td>
                  </tr>
                   {{/each}}                  
                   <tr>
                    <td>Доставка</td>
                    <td align="right">1</td>
                    <td align="right">{{delivery.deliveryAmount}}р</td>
                    <td align="right">{{delivery.deliveryAmount}}р</td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr>
          </tr>
        </tbody>
      </table>
      <table align="center" role="presentation" cellspacing="0" cellpadding="0" border="0"
        style="margin: auto;">
        <tbody>
          <tr>
            <td valign="top" class="bg_white" style="padding: 1em 2.5em 0 2.5em;">
              <table role="presentation" border="0" cellpadding="0" cellspacing="0" width="100%">
                <tbody>
                  <tr>
                    <td style="text-align: left;">
                      <h2 class="margin-end-header">Оплата</h2>
                    </td>
                  </tr>
                  <tr>
                    <td>{{delivery.paymentString}}</td>
                    <td>{{delivery.hitBackMessage}}</td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr>
          </tr>
        </tbody>
      </table>
      <table align="center" role="presentation" cellspacing="0" cellpadding="0" border="0"
        style="margin: auto;">
        <tbody>
          <tr>
            <td valign="top" class="bg_white" style="padding: 1em 2.5em 1em 2.5em;">
              <table role="presentation" border="0" cellpadding="0" cellspacing="0" width="100%">
                <tbody>
                  <tr>
                    <td style="text-align: left;">
                      <h2 class="margin-end-header">Итого:</h2>
                    </td>
                    <td style="text-align: right;">
                      <h2>{{orderTotalPrice}}р</h2>
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </center>
</body>

